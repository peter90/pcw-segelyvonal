// Resize function by -mano [http://crxforum.flix.hu/]
function m_resize() {
	maxszelesseg=446;
	for (i=0 ; i < document.images.length ; i++) {
		szelesseg = document.images[i].width;
		magassag = document.images[i].height;
		if (szelesseg>maxszelesseg){
			szorzo=maxszelesseg/szelesseg;
			ujszelesseg=Math.round(szelesseg*szorzo);
			ujmagassag=Math.round(magassag*szorzo);
			document.images[i].width=ujszelesseg;
			document.images[i].height=ujmagassag;
		}
	}
	return true;
}

